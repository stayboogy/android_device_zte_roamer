#!/system/bin/sh

# so you can run whatever you like at boot
# use a root file manager to edit this file
# with whatever init service or task you like
# and it will run at boot

rm /data/system/dropbox/*
rm /data/tombstones/*
echo 600000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 
echo 600000 > /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 
